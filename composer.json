{
    "name": "rhukster/filament-geosearch",

    "description": "Implementation of GeoSearch as a Filament Form Field",
    "keywords": [
        "filament",
        "form",
        "field",
        "leaflet",
        "geosearch"
    ],
    "homepage": "https://github.com/rhukster/filament-geosearch",
    "require": {
        "php": "^8.0",
        "filament/forms": "^2.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rhukster\\FilamentGeoSearch\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andy Miller",
            "email": "rhuk@rhuk.net"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Rhukster\\FilamentGeoSearch\\FilamentGeoSearchProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
